{
  "expo": {
    "name": "Seiri",
    "slug": "seiri",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/white-logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/white-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f0f1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.seiri.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/white-logo.png",
        "backgroundColor": "#0f0f1a"
      },
      "package": "com.seiri.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/white-logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "07e7481e-24d2-4e68-970a-cfc5ff1f0ff0"
      }
    }
  }
}
